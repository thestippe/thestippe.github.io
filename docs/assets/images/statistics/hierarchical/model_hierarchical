digraph {
	mu [label="mu
~
Deterministic" shape=box]
	alpha [label="alpha
~
HalfNormal" shape=ellipse]
	beta [label="beta
~
HalfNormal" shape=ellipse]
	subgraph "clusterobs_id (4)" {
		y [label="y
~
Binomial" shape=ellipse style=filled]
		theta [label="theta
~
Beta" shape=ellipse]
		label="obs_id (4)" labeljust=r labelloc=b style=rounded
	}
	alpha -> mu
	beta -> mu
	alpha -> theta
	beta -> theta
	theta -> y
}
